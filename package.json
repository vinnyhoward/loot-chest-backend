{
  "name": "loot-chest-backend",
  "version": "1.0.50",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "prisma db push && prisma generate && bun run --watch src/index.ts",
    "build": "bun build",
    "start": "bun run src/index.ts",
    "prisma": "prisma",
    "generate": "prisma generate",
    "studio": "prisma studio",
